<Application x:Class="GoToGagarin.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GoToGagarin">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Icons.xaml" />

                <ResourceDictionary Source="Resources/CommonBrushes.xaml" />
                <ResourceDictionary Source="Resources/CommonConverters.xaml" />

                <ResourceDictionary Source="Resources/Styles/BorderStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/TextStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/TabControlStyle.xaml" />

                <ResourceDictionary Source="Resources/Template/ButtonTemplate.xaml" />
                <ResourceDictionary Source="Resources/Template/TextBoxTemplate.xaml" />

                <ResourceDictionary Source="/MainComponents;component/themes/generic.xaml" />
                <ResourceDictionary Source="/MapControlLib;component/Themes/Generic.xaml"/>
                <ResourceDictionary Source="Resources/DataTemplates/Popups.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>