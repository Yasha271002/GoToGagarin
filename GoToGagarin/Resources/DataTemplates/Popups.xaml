<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:GoToGagarin.ViewModel.Popup"
                    xmlns:popups1="clr-namespace:GoToGagarin.View.Popups">
    <DataTemplate DataType="{x:Type popups:ContentSliderPopupViewModel}">
        <popups1:ContentSliderPopup DataContext="{Binding}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type popups:InactivityPopupViewModel}">
        <popups1:InactivityPopup DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>